---
layout: layouts/base.vto
---

<div class="bg-white p-8 rounded-lg">
    <h1 class="font-bold text-2xl mb-8"># {{ tag }}</h1>
    <ul class="grid">
        {{ for page of search.pages(`'${tag}'`) }}
        <li><a href="{{ page.url }}">{{ page.title }}</a></li>
        {{ /for }}
    </ul>
</div>